<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
		font-weight: bold;
		}
	</ui:style>
	<ui:with field='res' type='hu.mygame.client.resources.Resources' />

	<g:DialogBox ui:field="dialog" text="Invitation!" modal="true"
		autoHideEnabled="false" title="">
		<g:VerticalPanel>
			<g:Label ui:field="label" text="Invitation from" />
			<g:HorizontalPanel>
				<g:Button ui:field="acceptButton" text="Accept" />
				<g:Button ui:field="declineButton" text="Decline" />
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:DialogBox>
</ui:UiBinder> 