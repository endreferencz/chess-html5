<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type="hu.mygame.client.players.PlayersPanel.MyStyle">
		.leftpanel {
			padding: 5px;
		}
		
		.player {
			cursor: pointer;
		}
		
		.player:hover {
			cursor: pointer;
			background-color: #CCCCCC;
		}
	</ui:style>
	<g:VerticalPanel addStyleNames="{style.leftpanel}">
		<g:Label text="Players:" />
		<g:VerticalPanel ui:field="verticalPanel">
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 